<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Authors>Ryan Harris</Authors>
    <Company />
    <Product>ValueTypes</Product>
    <Description>An easy-to-use implementation of Value Objects from DDD. The goal of this little library is to make it as easy as possible to use ValueTypes.</Description>
    <PackageProjectUrl>https://github.com/TheFellow/ValueTypes</PackageProjectUrl>
    <PackageTags>Value, ValueObject, ValueType</PackageTags>
    <PackageLicenseFile>README.md</PackageLicenseFile>
    <PackageIcon>ValueTypes.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\README.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="C:\util\TheFellow\ValueTypes\LICENSE.md" Link="LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\..\ValueTypes.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
